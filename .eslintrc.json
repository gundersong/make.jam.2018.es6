{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "id-length": 0,
    "new-cap": 0,
    "no-new": 0,
    "no-plusplus":0,
    "func-names": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0
  }
}
